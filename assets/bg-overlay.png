    }
  }

  // ✅ Sidebar navigation logic
  document.querySelectorAll('.sidebar-link').forEach(link =&gt; {
    link.addEventListener('click', async (e) =&gt; {
      const section = e.target.dataset.section;
      await loadComponent('#settings-role-section', <code>../components/settingsSection${section}.html</code>);
    });
  });

