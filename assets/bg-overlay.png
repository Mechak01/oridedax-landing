import { getUserFromToken, isAuthenticated, getAuthHeader } from '../utils/auth.js'; 
import { apiGet, apiPost } from '../api/api.js'; 
import { loadComponent } from '../utils/dom.js';

window.addEventListener('DOMContentLoaded', async () =&gt; {
