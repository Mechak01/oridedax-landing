      // Populate the preview section with profile data
      document.getElementById('user-name').textContent = profileUser.name || 'Not Set';
      document.getElementById('user-email').textContent = profileUser.email || 'Not Set';
      document.getElementById('user-location').textContent = profileUser.location || 'Not Set';
      document.getElementById('user-languages').textContent = (profileUser.languages || []).join(', ') || 'Not Set';
      document.getElementById('user-availability').textContent = profileUser.availability || 'Not Set';

